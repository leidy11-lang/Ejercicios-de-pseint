Proceso sin_titulo
	definir parteA , parteB Como Entero
	
	ESCRIBIR " programa que verifica una igualdad" ;
	escribir " escriba el primer termino de la igualdad" ;
	
	parteA <-- (5-3-2+6);
	escribir "escriba el  segundo termino de la igualdad"
	
	parteB <-- (-2+6+5-3) ;
	
	ESCRIBIR parteA;
	
	ESCRIBIR parteB;
	
	si parteA = parteB Entonces escribir " si es igualdad";
	SiNo escribir " no es igualdad";
		
		
	FinSi
	
FinProceso
