Algoritmo valor_menor
	definir a, b Como Entero
	a=0;
	b=0;
	escribir "determinar que valor es el menor"
	Escribir " digite el valor de a "
	leer a ;
	Escribir " digite el valor de b "
	leer b;
	si a=b entonces 
		Escribir " cometio un error "  
		sino 
	FinSi
	
	si (a<b) entonces 
		escribir " a es el menor "
	sino 
		Escribir " b es el menor "
		
	FinSi
FinAlgoritmo
